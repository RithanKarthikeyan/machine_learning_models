{
    "manifest_version": 3,
    "name": "Spam Email Detector",
    "version": "1.0",
    "description": "Detect spam emails using ML",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["http://127.0.0.1:8000/*"],
    "content_scripts": [
      {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  